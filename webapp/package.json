    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "reset-css": "^5.0.2",
    "sass": "^1.85.1",
    "superjson": "^2.2.2",
    "zod": "^3.24.2"
