{
  "name": "@bookkey/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.json",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
